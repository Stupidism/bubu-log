{
  "name": "bubu-log",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 1030",
    "build": "next build",
    "start": "next start -p 1030",
    "lint": "eslint .",
    "payload": "payload",
    "generate:api": "openapi-typescript openapi.yaml -o src/lib/api/openapi-types.d.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "db:push": "pnpm db:migrate",
    "db:migrate": "PAYLOAD_DB_PUSH=false NODE_OPTIONS='-r ./scripts/shims/next-env-default.cjs' tsx scripts/migrate-prisma-to-payload.ts",
    "db:migrate:prisma-to-payload": "pnpm db:migrate",
    "db:seed": "NODE_OPTIONS='-r ./scripts/shims/next-env-default.cjs' tsx scripts/seed.ts",
    "db:setup-admin": "NODE_OPTIONS='-r ./scripts/shims/next-env-default.cjs' tsx scripts/setup-admin.ts",
    "db:setup-credentials": "NODE_OPTIONS='-r ./scripts/shims/next-env-default.cjs' tsx scripts/setup-credentials.ts",
    "db:seed:test": "NODE_OPTIONS='-r ./scripts/shims/next-env-default.cjs' tsx scripts/seed-test-data.ts",
    "db:pull:preview-branch-env": "bash scripts/pull-neon-branch-env.sh",
    "db:migrate:prod": "bash scripts/migrate-production.sh",
    "fix:diaper-dates": "NODE_OPTIONS='-r ./scripts/shims/next-env-default.cjs' tsx scripts/fix-batch-date-diaper.ts",
    "backfill:diaper-endtime": "NODE_OPTIONS='-r ./scripts/shims/next-env-default.cjs' tsx scripts/backfill-diaper-endtime.ts",
    "test:e2e:run": "playwright test",
    "test:e2e": "bash scripts/run-e2e.sh",
    "test:e2e:ui": "bash scripts/run-e2e.sh --ui",
    "test:e2e:headed": "bash scripts/run-e2e.sh --headed",
    "test:e2e:debug": "bash scripts/run-e2e.sh --debug"
  },
  "dependencies": {
    "@bubu-log/log-ui": "workspace:*",
    "@bubu-log/ui": "workspace:*",
    "@hookform/resolvers": "^5.2.2",
    "@payloadcms/db-postgres": "3.77.0",
    "@payloadcms/next": "3.77.0",
    "@tanstack/react-query": "^5.90.19",
    "@vercel/blob": "^2.0.0",
    "agentation": "^1.3.2",
    "bcryptjs": "^3.0.3",
    "dayjs": "^1.11.19",
    "graphql": "^16.11.0",
    "lucide-react": "^0.562.0",
    "next": "16.2.0-canary.10",
    "next-auth": "5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "openapi-fetch": "^0.15.0",
    "openapi-react-query": "^0.5.1",
    "payload": "3.77.0",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@bubu-log/typescript-config": "workspace:*",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.2.0-canary.10",
    "openapi-typescript": "^7.10.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
